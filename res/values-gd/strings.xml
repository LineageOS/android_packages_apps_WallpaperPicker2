    <string name="configure_wallpaper">Roghainnean…</string>
    <!-- List item for deleting the current wallpaper [CHAR LIMIT=30] -->
    <string name="delete_live_wallpaper">Sguab às</string>
    <!-- Confirmation dialog. Shown after user selects to delete one wallpaper. [CHAR LIMIT=NONE] -->
    <string name="delete_wallpaper_confirmation">A bheil thu airson am pàipear-balla seo a sguabadh às on fhòn agad?</string>
    <!-- Content description of back button on the bottom action bar. [CHAR LIMIT=30] -->
    <string name="bottom_action_bar_back">Air ais</string>
    <!-- Content description of edit button on the bottom action bar. [CHAR LIMIT=30] -->
    <string name="bottom_action_bar_edit">Deasaich</string>
    <!-- TODO(b/154299462): Separate downloadable related actions from WallpaperPicker. -->
    <!-- Content description of download button on the bottom action bar. [CHAR LIMIT=30] -->
    <string name="bottom_action_bar_download">Luchdaich a-nuas</string>
    <!-- Content description of "slideshow wallpaper" button on the bottom action bar. [CHAR LIMIT=50] -->
    <string name="bottom_action_bar_slideshow_wallpaper">Pàipear-balla de shleamhnagan</string>
    <!-- Content description of apply button on the bottom action bar. [CHAR LIMIT=30] -->
    <string name="bottom_action_bar_apply">Cuir an sàs</string>
